{
  "name": "Find On Foursquare",
  "description": "Searches for selected text on Foursquare.",
  "version": "0.1",
  "manifest_version": 2,
  "background": {
    "persistent": false,
    "scripts": ["main.js"]
  },
  "icons": {
    "16": "assets/icon-16.png",
    "32": "assets/icon-32.png",
    "48": "assets/icon-48.png",
    "128": "assets/icon-128.png"
  },
  "permissions": ["contextMenus", "tabs"]
}
